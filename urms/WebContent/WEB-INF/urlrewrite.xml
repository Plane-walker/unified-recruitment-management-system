<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE urlrewrite
    PUBLIC "-//tuckey.org//DTD UrlRewrite 4.0//EN"
    "http://www.tuckey.org/res/dtds/urlrewrite4.0.dtd">
<urlrewrite>
    <rule>
        <from>^/login_app$</from>  
        <to type="forward">/login_app.jsp</to>
    </rule>
    <rule>
        <from>^/login_com$</from>  
        <to type="forward">/login_com.jsp</to>
    </rule>
    <rule>
        <from>^/main$</from>  
        <to type="forward">/main.jsp</to>
    </rule>
    <rule>
        <from>^/register_app$</from>  
        <to type="forward">/register_app.jsp</to>
    </rule>
    <rule>
        <from>^/register_com$</from>  
        <to type="forward">/register_com.jsp</to>
    </rule>
</urlrewrite>